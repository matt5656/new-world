boolean addTrainee(Trainee t) throws SQLException {
		try{
			Class.forName(driver);
			Connection con = DriverManager.getConnection(url,user,pass);
			PreparedStatement pstmt = con
					.prepareStatement("insert into Trainee_Dtls values(?,?,?,?)");
			
			
			pstmt.setInt(1, t.getEmpid());
			pstmt.setString(2, t.getName());
			pstmt.setString(3, t.getBatchNum());
			pstmt.setString(4, t.getLgNum());
			
			}
			catch (Exception e) {
				
			}
			finally
			{
				if(pstmt!=null)
					pstmt.close();
					if(con!=null)
				con.close();
			}
			
			int no=pstmt.executeUpdate();
			if(no==1)
				return true;
			return false;	
		}
